{
  "expo": {
    "name": "FixiGo",
    "slug": "fixigo",
    "owner": "janabuabu2004",
    "scheme": "fixigo",
    "version": "1.0.0",
    "newArchEnabled": true,
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "userInterfaceStyle": "dark",
    "platforms": ["ios", "android", "web"],
    "splash": {
      "image": "./assets/images/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#050A30"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.yourcompany.yourappname",
      "buildNumber": "1.0.0",
      "googleServicesFile": "./google-services.json",
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to scan QR codes.",
        "NSLocationWhenInUseUsageDescription": "This app uses your location to show nearby places."
      }
    },
    "android": {
      "package": "com.anonymous.fixigo",
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo.png",
        "backgroundColor": "#050A30"
      }
    },
    "web": {
      "favicon": "./assets/images/logo.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-video",
      [
        "react-native-fbsdk-next",
        {
          "appID": "1294018571840644",
          "clientToken": "98c70fef86c3f9776846e4c63370d199",
          "displayName": "FixiGo",
          "scheme": "fb1294018571840644",
          "advertiserIDCollectionEnabled": false,
          "autoLogAppEventsEnabled": false,
          "isAutoInitEnabled": true,
          "iosUserTrackingPermission": "This identifier will be used to deliver personalized ads to you."
        }
      ],
      "@react-native-firebase/app",
      "@react-native-firebase/auth",
      [
        "expo-build-properties",
        {
          "ios":{
            "useFrameWorks": "static"
          }
        }
      ]
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "0d732cd8-7227-4303-b6fe-af918aceac6d"
      }
    }
  }
}
      
